{
	"manifest_version": 2,
	"name": "Chrome4Net.Echo",
	"version": "0.1.0",
	"key": "",
	"description": "An example extension",
	"icons": {
		"48": "icons/echo-48x48.png",
		"32": "icons/echo-32x32.png",
		"24": "icons/echo-24x24.png"
	},
	"content_scripts": [
		{
			"matches": [ "http://*/*", "https://*/*", "file://*/*" ],
			"js": [ "scripts/content.js" ]
		}
	],
	"background": {
		"scripts": [ "scripts/background.js" ],
		"persistent": false
	},
	"permissions": [
		"nativeMessaging"
	]
}
